{
    "name": "twentyfourhoursmedia/views-work",
    "description": "plugin for handling pageviews",
    "type": "craft-plugin",
    "version": "2.1.3",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "views work",
        "views-work"
    ],
    "support": {
        "docs": "https://github.com/24hoursmedia-craftcms/views-work/blob/master/README.md",
        "issues": "https://github.com/24hoursmedia-craftcms/views-work/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "24hoursmedia",
            "homepage": "https://www.24hoursmedia.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "craftcms/cms": "^4.0.0.1",
        "jaybizzle/crawler-detect": "^1.2"
    },
    "autoload": {
        "psr-4": {
          "twentyfourhoursmedia\\viewswork\\": "src/"
        }
    },
    "extra": {
        "name": "Views Work",
        "handle": "views-work",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/twentyfourhoursmedia/views-work/master/CHANGELOG.md",
        "components": {
            "viewsWorkService": "twentyfourhoursmedia\\viewswork\\services\\ViewsWorkService"
        },
        "class": "twentyfourhoursmedia\\viewswork\\ViewsWork"
    }
}
